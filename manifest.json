{
    "name": "Figma Chrome Proxy",
    "manifest_version": 3,
    "description": "解决无法访问 Figma，或者资源加载不出来的情况",
    "version": "1.0.6",
    "permissions": [
      "proxy",
      "activeTab",
      "storage"
    ],
    "host_permissions": [
      "*://*.figma.com/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }